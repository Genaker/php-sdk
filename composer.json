{
    "name": "blackfire/php-sdk",
    "description": "Blackfire.io PHP SDK",
    "keywords": ["profiler", "xhprof", "performance", "uprofiler"],
    "license": "MIT",
    "authors": [
        {
            "name": "Blackfire.io",
            "email": "support@blackfire.io"
        }
    ],
    "require": {
         "php": ">=5.2.0"
    },
    "suggest": {
        "ext-blackfire": "Is a feature enhanced C version of this package",
        "ext-xhprof": "XHProf is required as a fallback"
    },
    "autoload": {
        "files": [
            "src/autostart.php"
        ],
        "psr-0": {
            "Blackfire": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
